package multiThreading;

public class SynchronizedBlock {

	
	public void printLine() 
    { 
        for (int i = 0; i < 5; i++) 
        { 
            System.out.println(i); 
            try
            { 
                Thread.sleep(500); 
            } 
            catch (Exception e) 
            { 
                System.out.println(e); 
            } 
        } 
    } 
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
